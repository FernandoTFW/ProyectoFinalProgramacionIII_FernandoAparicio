<UserControl x:Class="InterfazCouchProgramacion.ventanas.uscMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InterfazCouchProgramacion.ventanas"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <StackPanel VerticalAlignment="Center">

            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Button Content="Registrar Equipos" Width="120" Height="120" Margin="5" Click="Button_Click_1"></Button>
                <Button Content="Registrar Integrantes" Width="120" Height="120" Margin="5" Click="Button_Click"></Button>
                <Button x:Name="btnCategoria" Content="Registrar Categorias" Width="120" Height="120" Margin="5" Click="btnCategoria_Click"></Button>
                <Button x:Name="btnEquipos" Content="Ver Equipos" Width="120" Height="120" Margin="5" Click="btnEquipos_Click"></Button>
            </StackPanel>
        </StackPanel>

    </Grid>
</UserControl>
